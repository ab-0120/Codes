///////////////////////////   FCFS
#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n;
    scanf("%d",&n);
    
    int arr[n];
    for(int i=0;i<n;i++)
        scanf("%d",&arr[i]);
        
    int pos;
    scanf("%d",&pos);
    
    int total=0;
    float avg=0.0;
    for(int i=0;i<n;i++)
    {
       total += abs(pos-arr[i]);
       pos=arr[i];
    }

    avg=total/n;
    printf("%d ",total);
    printf("%.2f",avg);
    return 0;
}
